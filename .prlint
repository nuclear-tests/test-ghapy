{
    "include": {},
    "checks": {
        "commit": {
            "Always true test": "true",
            "Not more than 1 file changed": "commit.files.length < 2",
            "The name of the file changed must not be .prlint": [
                "let file = commit.files[0].filename",
                "let author = commit.commit.author.name",
                "return (file != '.prlint' || author == 'Chieze Franklin');"
            ]
        },
        "pr": {
            "Always true test ": "true"
        }
    }
}
